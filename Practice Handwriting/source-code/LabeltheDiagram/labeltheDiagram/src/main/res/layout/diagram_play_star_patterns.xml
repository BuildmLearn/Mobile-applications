<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" xmlns:app="http://schemas.android.com/apk/res/com.example.labelthediagram">

    <RelativeLayout
        android:id="@+id/scoreTab"
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@color/tab_color" >

        <View
            android:id="@+id/view1"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:background="@color/appBg_color" />

        <com.buildmlearn.labeldiagram.widget.CustomFontManager
            android:id="@+id/score"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/scoreTxt"
            android:layout_alignBottom="@+id/scoreTxt"
            android:layout_marginLeft="18dp"
            android:layout_toRightOf="@+id/scoreTxt"
            android:text="0"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffff"
            app:typeface="roboto_thin" />

        <com.buildmlearn.labeldiagram.widget.CustomFontManager
            android:id="@+id/complete_ratio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/complatedTxt"
            android:layout_alignBottom="@+id/complatedTxt"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/complatedTxt"
            android:text=" 0%"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffff"
            app:typeface="roboto_thin" />

        <com.buildmlearn.labeldiagram.widget.CustomFontManager
            android:id="@+id/complatedTxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:text="Complete :"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffff" 
            app:typeface="roboto_thin"/>

        <com.buildmlearn.labeldiagram.widget.CustomFontManager
            android:id="@+id/scoreTxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/complete_ratio"
            android:layout_alignBottom="@+id/complete_ratio"
            android:layout_marginLeft="24dp"
            android:layout_toRightOf="@+id/view1"
            android:text="Score   :"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffff" 
            app:typeface="roboto_thin"/>
    </RelativeLayout>

    <ImageView
        android:id="@+id/mainDiagramStarPatterns"
        android:layout_width="320dp"
        android:layout_height="300dp"
        android:layout_below="@+id/scoreTab"
        android:layout_centerHorizontal="true"
        android:src="@drawable/stars" />

    <ImageView
        android:id="@+id/tagHolder"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:adjustViewBounds="true"
        android:src="@drawable/holder" />
    
    <ImageView
        android:id="@+id/libraBlb"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/mainDiagramStarPatterns"
        android:layout_alignTop="@+id/mainDiagramStarPatterns"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="3dp"
        android:src="@drawable/marker_white" />
    
    <ImageView
        android:id="@+id/little_dripperBlb"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/mainDiagramStarPatterns"
        android:layout_alignTop="@+id/mainDiagramStarPatterns"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="3dp"
        android:src="@drawable/marker_white" />
    
    <ImageView
        android:id="@+id/big_dripperBlb"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignRight="@+id/mainDiagramStarPatterns"
        android:layout_alignTop="@+id/mainDiagramStarPatterns"
        android:layout_marginRight="70dp"
        android:layout_marginTop="3dp"
        android:src="@drawable/marker_white" />
    
    <ImageView
        android:id="@+id/scorpioBlb"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignRight="@+id/mainDiagramStarPatterns"
        android:layout_alignTop="@+id/mainDiagramStarPatterns"
        android:layout_marginRight="45dp"
        android:layout_marginTop="115dp"
        android:src="@drawable/marker_white" />
    
    <ImageView
        android:id="@+id/geminiBlb"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignRight="@+id/mainDiagramStarPatterns"
        android:layout_alignTop="@+id/mainDiagramStarPatterns"
        android:layout_marginRight="145dp"
        android:layout_marginTop="155dp"
        android:src="@drawable/marker_white" />
    
    <ImageView
        android:id="@+id/leoBlb"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignRight="@+id/mainDiagramStarPatterns"
        android:layout_alignBottom="@+id/mainDiagramStarPatterns"
        android:layout_marginRight="45dp"
        android:layout_marginBottom="45dp"
        android:src="@drawable/marker_white" />
    
    <ImageView
        android:id="@+id/dracoBlb"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignRight="@+id/mainDiagramStarPatterns"
        android:layout_alignBottom="@+id/mainDiagramStarPatterns"
        android:layout_marginRight="153dp"
        android:layout_marginBottom="32dp"
        android:src="@drawable/marker_white" />
    
    <ImageView
        android:id="@+id/orianBlb"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/mainDiagramStarPatterns"
        android:layout_alignBottom="@+id/mainDiagramStarPatterns"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="65dp"
        android:src="@drawable/marker_white" />
    
    <ImageView
        android:id="@+id/innerTagHolder"
        android:layout_width="345dp"
        android:layout_height="125dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/tagHolder"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="50dp"
        android:scaleType="fitXY"
        android:src="@drawable/trans_inner_holder" />
    
    <TextView
        android:id="@+id/orianTag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/innerTagHolder"
        android:layout_alignTop="@+id/innerTagHolder"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/custom_textview"
        android:paddingBottom="3dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="3dp"
        android:text="orian"
        android:textColor="#ffff" />
    
    <TextView
        android:id="@+id/scorpioTag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/innerTagHolder"
        android:layout_alignTop="@+id/innerTagHolder"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/custom_textview"
        android:paddingBottom="3dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="3dp"
        android:text="scorpio"
        android:textColor="#ffff" />
    
    <TextView
        android:id="@+id/little_dripperTag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/innerTagHolder"
        android:layout_alignTop="@+id/innerTagHolder"
        android:layout_marginLeft="157dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/custom_textview"
        android:paddingBottom="3dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="3dp"
        android:text="little dripper"
        android:textColor="#ffff" />
    
    <TextView
        android:id="@+id/leoTag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/innerTagHolder"
        android:layout_alignTop="@+id/innerTagHolder"
        android:layout_marginLeft="265dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/custom_textview"
        android:paddingBottom="3dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="3dp"
        android:text="leo"
        android:textColor="#ffff" />
    
    <TextView
        android:id="@+id/libraTag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/innerTagHolder"
        android:layout_alignTop="@+id/innerTagHolder"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="45dp"
        android:background="@drawable/custom_textview"
        android:paddingBottom="3dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="3dp"
        android:text="libra"
        android:textColor="#ffff" />
    
    <TextView
        android:id="@+id/dracoTag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/innerTagHolder"
        android:layout_alignTop="@+id/innerTagHolder"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="45dp"
        android:background="@drawable/custom_textview"
        android:paddingBottom="3dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="3dp"
        android:text="draco"
        android:textColor="#ffff" />
    
    <TextView
        android:id="@+id/big_dripperTag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/innerTagHolder"
        android:layout_alignTop="@+id/innerTagHolder"
        android:layout_marginLeft="138dp"
        android:layout_marginTop="45dp"
        android:background="@drawable/custom_textview"
        android:paddingBottom="3dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="3dp"
        android:text="big dripper"
        android:textColor="#ffff" />
    
    <TextView
        android:id="@+id/geminiTag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/innerTagHolder"
        android:layout_alignTop="@+id/innerTagHolder"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="45dp"
        android:background="@drawable/custom_textview"
        android:paddingBottom="3dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="3dp"
        android:text="gemini"
        android:textColor="#ffff" />
    
    

</RelativeLayout>