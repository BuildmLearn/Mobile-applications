<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomFontManager">
        <attr name="typeface" />
    </declare-styleable>

    <attr name="typeface" format="enum">
        <enum name="roboto_thin" value="0" />
        <enum name="roboto_light" value="1" />
        <enum name="roboto_regular" value="2" />
        <enum name="roboto_bold" value="3" />
        <enum name="roboto_condensed_light" value="4" />
        <enum name="roboto_condensed_light_italic" value="5" />
        <enum name="roboto_condensed_regular" value="6" />
    </attr>

</resources>